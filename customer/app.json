{
  "expo": {
    "name": "customer",
    "slug": "customer",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "http",
            "host": "*.myapp.io",
            "pathPrefix": "/records"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}